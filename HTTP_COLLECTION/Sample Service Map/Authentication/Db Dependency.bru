meta {
  name: Db Dependency
  type: http
  seq: 3
}

post {
  url: {{baseUrl}}/services/:id/dependency
  body: json
  auth: none
}

params:path {
  id: {{api_id}}
}

body:json {
  {
    "id": "{{db_id}}"
  }
}

vars:pre-request {
  api_id: 
  db_id: 
}

script:pre-request {
  bru.setVar('api_id', bru.getEnvVar('auth_api_id'))
  bru.setVar('db_id', bru.getEnvVar('auth_db_id'))
  
  
}

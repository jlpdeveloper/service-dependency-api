meta {
  name: Cache
  type: http
  seq: 4
}

post {
  url: {{baseUrl}}/services
  body: json
  auth: none
}

body:json {
  {
    "name": "Warehouse Cache",
    "type": "cache",
    "description": "warehouse redis cache",
    "url": "{{$randomUrl}}"
  }
}

script:post-response {
  if (res.status == 201) {
    bru.setEnvVar('warehouse_cache_id', res.body.id, { persist: false})
  }
}
